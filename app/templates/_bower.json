{
  "name": "<%= slugifiedAppName %>",
  "description": "<%= appDescription %>",
  "version": "0.0.1",
	"install" : {
        "path"  : {
            "js": "app/assets/libs",
            "css": "app/assets/css"
        }
    },
  "dependencies": {
    "json3": "3.3.1",
    "es5-shim": "3.1.0",<% if (angularBootstrap) { %>
  	"bootstrap": "3.3.6",
    "jquery": "2.2.0",
    "angular-touch": ">=1.3.*",
    "angular-bootstrap": "~0.14.3",<% } %>
	"angular": ">=1.3.*",
  	"angular-aria": ">=1.3.*",
	"angular-mocks": ">=1.3.*",<% if (angularCookies) { %>
	"angular-cookies": ">=1.3.*",<% } if (angularAnimate) { %>
	"angular-animate": ">=1.3.*",<% } if (angularSanitize) { %>
	"angular-sanitize": ">=1.3.*",<% } %>
  	"angular-resource": ">=1.3.*",<% if (angularMaterial) { %>
	"angular-material-icons": "~0.6.0",
	"angular-messages": ">=1.3.*",
	"angular-material": ">=1.0.1",<% } %>
	"angular-ui-router": "~0.2.10"
  },
  "resolutions": {
    "angular": ">=1.3.*"
  }
}
